    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "16.2.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pre-commit": "^1.0.10",
    "prettier": "^3.6.2",
    "shell-quote": "^1.8.1",
