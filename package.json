    "gsap": "^3.13.0",
    "katex": "^0.16.23",
    "lenis": "^1.3.11",
    "next": "^15.5.4",
    "p5": "^2.0.5",
    "prismjs": "^1.29.0",
    "qiniu": "^7.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-jsx-parser": "github:EliteByte/react-jsx-parser#develop",
    "ts-node": "^10.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
